{
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/spa",
  "routes": [
    {
      "src": "/mysql-vehicles",
      "dest": "/index.html"
    },
    {
      "src": "/builder-inventory", 
      "dest": "/index.html"
    },
    {
      "src": "/woocommerce-vehicles",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}